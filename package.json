{
  "name": "flux-bench",
  "version": "0.0.1",
  "description": "flux bench",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "pull": "git checkout master && git pull origin master",
    "push": "git push origin master --tags && npm publish",
    "dev": "npm run build -- --watch",
