    "replay",
    "flux",
    "elm"
  ],
  "files": [
    "*.map",
    "*.js",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "babel-runtime": "^5.8.24",
    "baobab": ">=2.0.0-dev15",
    "immutable-di": "^1.3.31"
